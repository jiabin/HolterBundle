{
    "name":        "jiabin/holter-bundle",
    "type":        "symfony-bundle",
    "description": "Service status bundle for Symfony2",
    "keywords":    ["status", "monitor", "health"],
    "homepage":    "https://github.com/jiabin/HolterBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Eymen Gunay",
            "email": "eymen@egunay.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.1,<2.6-dev",
        "doctrine/common": ">=2.1.3",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev",
        "components/html5-boilerplate": "*",
        "components/bootstrap": "*",
        "twig/extensions": "*",
        "craue/formflow-bundle": "~2.0",
        "simplepie/simplepie": "~1.3"
    },
    "target-dir": "Jiabin/HolterBundle",
    "autoload": {
        "psr-0": {
            "Jiabin\\HolterBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
