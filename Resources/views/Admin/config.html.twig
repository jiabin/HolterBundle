{% extends "JiabinHolterBundle:Base:layout.html.twig" %}

{% block subtitle %}admin:config{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-md-12">
            <form action="{{ app.request.uri }}" role="form" method="post">
                {{ form_widget(form) }}
                <div class="text-right">
                    <button class="btn btn-primary" type="submit">
                        <span class="glyphicon glyphicon-ok"></span>
                        &nbsp;Save
                    </button>
                </div>
            </form>
        </div>
    </div>
{% endblock %}